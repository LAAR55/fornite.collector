{
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "es5",
      "es2015.promise",
      "es2017",
    ],
    "target": "es5",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": false,
  },
  "exclude": [
    "node_modules"
  ],
}
